extends ../layout/app

block content
  .row#user-area
    .avatar.col-sm-12
      img.userImg(src="/images/user/operator.png", style="height:90px;width:90px;")
    ul.task-menu.list-unstyled.nav.navbar-nav
      li
        a(href="/")
          img(src="/images/ongoing.png")
          span 待办
      li.active
        img(src="/images/completed.png")
        span 已办结
      li
        a(href="/processes") 
          img(src="/images/create.png")
          span 受理
  .container
    .g-content
      .m-activity
        h3
          //span Completed Activities
          span 活动详情

        .col-md-11.col-md-offset-1(style="margin-top:20px")
          .panel.panel-primary
            .panel-heading 
              h3.panel-title #{instance.getProcess().getName()}
            .panel-body
              table.table.table-hover.table-bordered.table-stripped
                tbody
                  tr
                    td 业务编码
                    td #{map.get("CODE")}
                    td 审批年度
                    td #{map.get("SPND")}
                    td 申请人姓名
                    td #{map.get("SQRXM")}
                  tr
                    td 申请人身份证号
                    td #{map.get("SQRSFZH")}
                    td 申请人年龄
                    td #{map.get("SQRNL")}
                    td 申请人工作单位
                    td #{map.get("SQRDWMC")}
                  tr
                    td 配偶姓名
                    td #{map.get("POXM")}
                    td 配偶年龄
                    td #{map.get("PONL")}
                    td 配偶工作单位
                    td #{map.get("PODWMC")}
                  tr
                    td 家庭人数
                    td #{map.get("JTRS")}
